<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <package name="user" extends="struts-default">
        <action name="preCreateAssignment" class="controller.assignment.PreCreateAssignment">
            <result name="success">/adv/assignment/create-assignment.jsp</result>
            <result name="error">/error.jsp</result>      
        </action>
        
        <action name="createAssignment" class="controller.assignment.CreateAssignment">
            <result name="success">/adv/assignment/waiting-process.jsp</result>
            <result name="error">/adv/assignment/create-assignment.jsp</result>      
        </action>
        
        <action name="preListAssignment" class="controller.assignment.PreListAssignment">
            <result name="success">/adv/assignment/list-assignment.jsp</result>
            <result name="error">/error.jsp</result>      
        </action>
    </package>
</struts>