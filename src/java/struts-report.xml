<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <package name="user" extends="struts-default">
        <action name="preReportBySubmitted" class="controller.report.ReportStudent" method="preReportBySubmitted">
            <result name="success">/adv/report/report-student-submitted.jsp</result>
            <result name="error">/adv/report/report-student-submitted.jsp</result>      
        </action>
        <action name="reportBySubmitted" class="controller.report.ReportStudent" method="reportBySubmitted">
            <result name="success">/adv/report/report-student-submitted.jsp</result>
            <result name="error">/adv/report/report-student-submitted.jsp</result>      
        </action>
        
        <action name="preReportByMark" class="controller.report.ReportStudent" method="preReportByMark">
            <result name="success">/adv/report/report-student-mark.jsp</result>
            <result name="error">/adv/report/report-student-mark.jsp</result>      
        </action>
        <action name="reportByMark" class="controller.report.ReportStudent" method="reportByMark">
            <result name="success">/adv/report/report-student-mark.jsp</result>
            <result name="error">/adv/report/report-student-mark.jsp</result>      
        </action>
        
        <action name="preReportByType" class="controller.report.ReportStudent" method="preReportByType">
            <result name="success">/adv/report/report-student-type.jsp</result>
            <result name="error">/adv/report/report-student-type.jsp</result>      
        </action>
        <action name="reportByType" class="controller.report.ReportStudent" method="reportByType">
            <result name="success">/adv/report/report-student-type.jsp</result>
            <result name="error">/adv/report/report-student-type.jsp</result>      
        </action>
    </package>
</struts>